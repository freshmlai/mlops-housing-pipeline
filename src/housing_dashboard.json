{
  "dashboard": {
    "id": null,
    "title": "Housing Model Dashboard",
    "tags": ["housing", "mlops", "prometheus"],
    "timezone": "browser",
    "schemaVersion": 16,
    "version": 0,
    "refresh": "10s",
    "panels": [
      {
        "type": "graph",
        "title": "Prediction Count",
        "targets": [
          {
            "expr": "sum(predict_total)",
            "legendFormat": "Total Predictions",
            "refId": "A"
          }
        ],
        "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 }
      },
      {
        "type": "graph",
        "title": "Average Prediction Value",
        "targets": [
          {
            "expr": "avg(predict_value)",
            "legendFormat": "Avg Prediction",
            "refId": "B"
          }
        ],
        "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 }
      },
      {
        "type": "stat",
        "title": "MSE Score",
        "targets": [
          {
            "expr": "model_mse",
            "refId": "C"
          }
        ],
        "gridPos": { "x": 0, "y": 8, "w": 6, "h": 4 }
      },
      {
        "type": "stat",
        "title": "RMSE Score",
        "targets": [
          {
            "expr": "model_rmse",
            "refId": "D"
          }
        ],
        "gridPos": { "x": 6, "y": 8, "w": 6, "h": 4 }
      }
    ]
  },
  "overwrite": true
}
